<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.wifiscanner" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="networkinterface">
        <param name="android-package" value="com.albahra.plugin.networkinterface.networkinterface" />
    </feature>
    <feature name="WifiWizard2">
        <param name="android-package" value="wifiwizard2.WifiWizard2" />
        <param name="onload" value="true" />
    </feature>
    <name>Scan Wifi</name>
    <description>
        A sample Apache Cordova application that scans local wifi networks.
    </description>
    <author email="emrys@openenergymonitor.org" href="https://openenergymonitor.org">
        Emrys Roberts
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://localhost:3000" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
